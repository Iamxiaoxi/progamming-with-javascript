// the following codes create an array called clothes and an object called favCar.
var clothes = [];
clothes.push("socks");
clothes.push("boots");
clothes.push("ties");
clothes.push("shirts");
clothes.push("trousers");
clothes.pop();
clothes.push("shorts");
console.log(clothes[2]);

var favCar = {};
favCar.color = "maroon";
favCar.convertible = true;
console.log(favCar)